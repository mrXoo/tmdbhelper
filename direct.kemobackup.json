{    "name"              : "KEMO BACKUP",    "plugin"            : "plugin.video.kemobackup",	"is_resolvable"		: "true",    "priority" 			: 201,    "play_movie"        : [                        "plugin://plugin.video.kemobackup/",						{"title": ".*Movies"},						{"title": ".*Search Movies"},						{"keyboard": "{title}"},						{"title": ".*{year}"},						{"dialog": "Auto"}                        ],    "play_episode"    : [						"plugin://plugin.video.kemobackup/",						{"title": ".*TV Shows.*"},						{"title": ".*Search TV Shows.*"},						{"keyboard": "{showname}"},                        {"title": "{showname}"},						{"title": ".*{season}"},						{"title": ".*{episode}.*"}                        ]} 