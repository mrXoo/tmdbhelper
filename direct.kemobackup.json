{    "name"              : "KEMO BACKUP",    "plugin"            : "plugin.video.kemobackup",	"is_resolvable"		: "true",    "priority" 			: 201,    "play_movie"        : [                        "plugin://plugin.video.kemobackup/?mode=49&url=VOD&xtra=test_parm",						{"keyboard": "{name}"},						{"label": ".*{title}.*"}                        ],    "play_episode"    : [						"plugin://plugin.video.kemobackup/",						{"title": ".*TV Shows.*"},						{"title": ".*Search TV Shows.*"},						{"keyboard": "{showname}"},						{"label": "(?i)^(\\[.*\\])?{showname}(\\[.*\\])?$"},												{"label": ".*Season.{season}"},						{"title": "(?i).*{showname}.*S.*{season}E.*{episode}|(?i).*S.*{season}E.*{episode}.*{showname}|(?i).*{showname}.*{title}|Episode.*{episode}"}                        ]} 