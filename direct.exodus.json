{    "name"              : "Exodus PAID VOD",    "plugin"            : "plugin.video.exodusv2",	"is_resolvable"		: "true",    "priority" 			: 500,    "play_movie"        : [                        "plugin://plugin.video.exodusv2/",						{"title": ".*Movies"},						{"title": ".*Search Movies"},						{"keyboard": "{title}"},						{"title": "(?i).*{title}"}                        ],    "play_episode"    : [                        "plugin://plugin.video.exodusv2/",						{"title": ".*TV Shows"},						{"title": ".*Search TV Shows"},						{"keyboard": "{showname}"},                        {"title": "{showname}"},						{"title": "Season.*{season}"},						{"title": "Episode.*{episode}"}                        ]}