{    "name"              : "KEMO -> THE OATH",    "plugin"            :						[						"plugin.video.kemo",						"plugin.video.theoath"						],	"is_resolvable"		: "true",    "priority" 			: 200,    "play_movie"        : [                        "plugin://plugin.video.kemo/?mode=49&url=VOD",						{"keyboard": "{name}"},						{"label": ".*{title}.*"}                        ],    "play_episode"    : [						"plugin://plugin.video.kemo/",						{"title": ".*"},						{"title": ".*TV Shows.*"},						{"title": ".*Search TV Shows.*"},						{"keyboard": "{showname}"},												{"label": "(?i)^(\\[.*\\])?{showname}(\\[.*\\])?$|(?i)^(\\[.*\\])?{showname}.*"},						{"label": ".*Season.{season}|.*Season.{season}.*"},												{"title": "(?i).*{showname}.*S.*{season}E.*{episode}|(?i).*S.*{season}E.*{episode}.*{showname}|(?i).*{showname}.*{title}|Episode.*{episode}|Episode.{episode}"}                        ],    "fallback": {    "play_movie": "direct.theoath.json play_movie",    "play_episode": "direct.theoath.json play_episode"    }}