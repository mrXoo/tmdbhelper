{    "name"              : "KEMO",    "plugin"            : "plugin.video.kemo",	"is_resolvable"		: "true",    "priority" 			: 200,    "play_movie"        : [                        "plugin://plugin.video.kemo/?mode=49&url=VOD",						{"keyboard": "{name}"},						{"label": ".*{title}.*"}                        ],    "play_episode"    : [                        "plugin://plugin.video.kemo/",						{"title": ".*TV Shows.*"},						{"title": ".*Search TV Shows.*"},						{"keyboard": "{showname}"},						{"label": "(?i)^(\\[.*\\])?{showname}(\\[.*\\])?$"},												{"label": ".*Season.{season}"},						{"title": "(?i).*{showname}.*S.*{season}E.*{episode}|(?i).*S.*{season}E.*{episode}.*{showname}|(?i).*{showname}.*{title}|Episode.*{episode}"}                        ]}