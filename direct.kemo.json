{    "name"              : "KEMO",    "plugin"            : "plugin.video.kemo",	"is_resolvable"		: "true",    "priority" 			: 500,    "play_movie"        : [                        "plugin://plugin.video.kemo/",						{"title": ".*Click Here.*"},						{"title": ".*Movies"},						{"title": ".*Search Movies"},						{"keyboard": "{title}"},						{"title": "(?i).*{title}"}                        ],    "play_episode"    : [                        "plugin://plugin.video.kemo/",						{"title": ".*Click Here.*"},						{"title": ".*TV Shows.*"},						{"title": ".*Search TV Shows.*"},						{"keyboard": "{showname}"},                        {"title": "{showname}"},						{"title": "Season.*{season}"},						{"title": "Episode.*{episode}"}                        ]}